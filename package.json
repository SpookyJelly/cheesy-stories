{
  "name": "chessy-stories",
  "version": "0.0.0",
  "description": "control your story's width like a chesse",
  "keywords": [
    "storybook-addons",
    "storybook-addons",
    "style",
    "appearance"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SpookyJelly/cheesy-stories"
  },
  "type": "module",
  "license": "MIT",
  "author": "SpookyJelly <zang9412@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "yarn clean",
    "dev": "vite build --watch --config vite.watch.config.ts",
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.1.6",
    "@storybook/addon-interactions": "8.1.6",
    "@storybook/addon-links": "8.1.6",
    "@storybook/blocks": "8.1.6",
    "@storybook/components": "8.1.6",
    "@storybook/core-events": "8.1.6",
    "@storybook/manager": "8.1.6",
    "@storybook/manager-api": "8.1.6",
    "@storybook/preview": "8.1.6",
    "@storybook/preview-api": "8.1.6",
    "@storybook/react": "8.1.6",
    "@storybook/react-vite": "8.1.6",
    "@storybook/test": "8.1.6",
    "@storybook/theming": "8.1.6",
    "@storybook/types": "8.1.6",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "auto": "^11.1.6",
    "boxen": "^7.1.1",
    "dedent": "^1.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "storybook": "8.1.6",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.13",
    "zx": "^8.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.ts"
    ],
    "previewEntries": [
      "src/preview.tsx"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "chessy-stories",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "re-resizable": "^6.9.17"
  }
}
